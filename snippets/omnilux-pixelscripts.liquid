<!-- MNTN Shopify DataLayer -->
{% if product %}
   <script type="text/javascript" async>
      let mntn_product_name = {{product.title | json}}
      let mntn_product_price = {{product.price | money_without_trailing_zeros | json}}
      let mntn_product_image = {{product.featured_image | json}}
      let mntn_product_sku = {{product.id | json}}     
      let mntn_product_in_stock = {{product.available | json}}
      let mntn_product_data = {{product | json}}
   </script>
{% endif %}
{% if cart.item_count > 0 %}
   <script type="text/javascript" async>
      let mntn_cart_quantity = {{cart.item_count | json}}
      let mntn_cart_value = {{cart.items_subtotal_price | money_without_trailing_zeros | json}}
      let mntn_cart_data = {{cart | json}}
   </script>
{% endif %}
    <!-- End MNTN Shopify DataLayer -->
