      "blocks": {
        "usp_cdBm37": {
          "type": "usp",
          "settings": {
            "hoox_how_to_image": "shopify:\/\/shop_images\/PDPs_23.png",
            "hoox_how_to_text": "<strong>Kickstart with Enzyme Powder Cleanser<\/strong>: Mix the cleanser with a little bit of water to create a lather (less water for more exfoliation or more water for a foamier cleanse). Massage onto your face, steering clear of the eyes, rinse, and pat dry."
          }
        },
        "usp_7iDcfC": {
          "type": "usp",
          "settings": {
            "hoox_how_to_image": "shopify:\/\/shop_images\/PDPs_18.png",
            "hoox_how_to_text": "<strong>Amplify with Hyaluronic Acid Serum<\/strong>: Before starting your LED treatment, pump a drop onto your hands and massage onto the treatment area."
          }
        },
        "usp_4YradN": {
          "type": "usp",
          "settings": {
            "hoox_how_to_image": "shopify:\/\/shop_images\/PDPs_21.png",
            "hoox_how_to_text": "<strong>Activate LED<\/strong>: Turn on the anti-aging power with your Omnilux Contour or Omnilux Men LED session while the Hyaluronic Acid Serum deeply hydrates your skin. Massage any excess serum into your skin afterward."
          }
        },
        "usp_d6iGza": {
          "type": "usp",
          "settings": {
            "hoox_how_to_image": "shopify:\/\/shop_images\/PDPs_22.png",
            "hoox_how_to_text": "<strong>Finish strong with Peptide Concentrate<\/strong>: After your LED session, dispense a small drop into your hands and gently massage onto the treatment area. Follow with moisturizer (optional)."
          }
        }
      },
